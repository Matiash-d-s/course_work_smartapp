{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c && node index.js",
    "dev": "concurrently --kill-others \"npm run dev-front\" \"npm run dev-back\"",
    "dev-front": "rollup -c -w",
    "dev-back": "nodemon index.js",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@sberdevices/assistant-client": "^2.11.9",
    "@sberdevices/plasma-tokens": "1.5.1",
    "concurrently": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "dialute": "0.0.21",
    "express": "^4.17.1",
    "json-to-css": "^0.1.0",
    "sirv-cli": "^1.0.0"
  }
}
